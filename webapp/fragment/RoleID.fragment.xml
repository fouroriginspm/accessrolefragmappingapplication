<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogRole"
		title="Select Role ID"
		items="{appModel>/role}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
        multiSelect="true"
		cancel=".onValueHelpClose">
		<StandardListItem
			
			title="{appModel>ROLE_ID}"
			description="{appModel>ROLE_CODE}" />
	</SelectDialog>
</core:FragmentDefinition>